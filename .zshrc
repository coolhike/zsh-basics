# zsh configuration file

# Load aliases
source ~/zsh-basics/aliases.zsh

# Load functions
source ~/zsh-basics/functions.zsh

# Set custom theme
ZSH_THEME="custom_theme"
source ~/zsh-basics/themes/custom_theme.zsh-theme

# Set other Zsh options
setopt autocd
setopt correct
setopt nocaseglob

